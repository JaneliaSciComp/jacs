<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:net.sf.taverna:wfDefinition:8f37f834-7ec5-40c1-a235-292cf7dd3ffd" author="" title="blastStatusWorkflow" />
  <s:processor name="getBlastStatus">
    <s:defaults>
      <s:default name="getBlastStatus">1258939856885645691</s:default>
    </s:defaults>
    <s:arbitrarywsdl>
      <s:wsdl>http://erato.jcvi.org:8080/compute-compute/Blast?wsdl</s:wsdl>
      <s:operation>getBlastStatus</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:link source="getBlastStatus:getBlastStatusResponse" sink="statusOutput" />
  <s:sink name="statusOutput" />
</s:scufl>

