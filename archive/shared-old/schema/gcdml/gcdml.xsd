<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://gensc.sf.net/gdml" xmlns="http://www.w3.org/2001/XMLSchema"
    xmlns:gml="http://www.opengis.net/gml" xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:gdml="http://gensc.sf.net/gdml" elementFormDefault="qualified" version="1.4">

    <annotation>
        <documentation xml:lang="en"> This schema is the implementation of the Genomic Contextual
            Data Marjup Language. The latest version of the schema can be found at :
            http://gensc.sf.net </documentation>
    </annotation>

    <import namespace="http://www.opengis.net/gml"
        schemaLocation="../gml-3.1.1/gml.xsd" />

    <include schemaLocation="./basicTypes.xsd" />
    <include schemaLocation="./environmentalParameters.xsd" />
    <include schemaLocation="./sourceData.xsd" />

    <element name="nasReports">
        <annotation>
            <documentation xml:lang="en">Nucleic Acid Sequence Report. The root element of all
                MIGS/MIMS compliant reports as standardized by Genomic Standards
            Consortium.</documentation>
        </annotation>
        <complexType>
            <sequence>
                <!-- Can be any substitution of _Report  -->
                <element ref="gdml:_NasReport" minOccurs="1" maxOccurs="unbounded" />
            </sequence>
        </complexType>
    </element>


    <element name="originalSample" type="gdml:SourceSampleType" />

    <complexType name="SourceSampleType">
        <annotation>
            <documentation xml:lang="en"> The environmental source description of the original,
                physical material or organismal sample collected from the environment.
            </documentation>
        </annotation>
        <sequence>
            <choice>
                <element name="collection" type="gdml:SampleCollectionType" />
                <choice>
                    <element ref="gdml:physicalMaterial" />
                    <element ref="gdml:organismalMaterial" />
                </choice>
            </choice>
        </sequence>
    </complexType>

    <complexType name="SourceSampleMetagenomicType">
        <sequence>
            <choice>
                <element name="collection" type="gdml:SampleCollectionMetagenomicType"> </element>
                <choice>
                    <element name="physicalMaterial" type="gdml:MetagenomePhysicalSampleType" />
                    <element ref="gdml:organismalMaterial" />
                </choice>
            </choice>
        </sequence>
    </complexType>

    <attribute name="criteria" type="gdml:CollectionCriteriaEnumType" />

    <element name="sampleCollection" type="gdml:SampleCollectionType" />

    <complexType name="SampleCollectionType">
        <choice maxOccurs="unbounded">
            <element ref="gdml:organismalMaterial" />
            <element ref="gdml:physicalMaterial" />
        </choice>
        <attribute ref="gdml:criteria" />
    </complexType>

    <complexType name="SampleCollectionMetagenomicType">
        <choice maxOccurs="unbounded">
            <element ref="gdml:organismalMaterial" />
            <element name="physicalMaterial" type="gdml:MetagenomePhysicalSampleType" />
        </choice>
        <attribute ref="gdml:criteria" />
    </complexType>


    <simpleType name="CollectionCriteriaEnumType">
        <restriction base="token">
            <enumeration value="pooled" />
            <enumeration value="replicates" />
            <enumeration value="different" />
            <enumeration value="single" />
        </restriction>
    </simpleType>


    <complexType name="AbstractMaterialType" abstract="true">
        <sequence>
            <element ref="gdml:samplingTime" minOccurs="1" />
            <element ref="gdml:_SampleLocation" minOccurs="1" />
            <element ref="gdml:_Habitat" minOccurs="1" />
        </sequence>
    </complexType>


    <element name="physicalMaterial" type="gdml:PhysicalMaterialType"
        substitutionGroup="gdml:_Material" />

    <complexType name="PhysicalMaterialType">
        <complexContent>
            <extension base="gdml:AbstractMaterialType">
                <sequence>
                    <element name="materialType" type="string" />
                    <element ref="gdml:amount" minOccurs="0" />
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <complexType name="MetagenomePhysicalSampleType">
        <complexContent>
            <extension base="gdml:AbstractMaterialType">
                <sequence>
                    <element name="materialType" type="string" />
                    <element ref="gdml:amount" minOccurs="1" />
                </sequence>
            </extension>
        </complexContent>
    </complexType>



    <element name="organismalMaterial" type="gdml:OrganismalMaterialType"
        substitutionGroup="gdml:_Material" />


    <complexType name="OrganismalMaterialType">
        <complexContent>
            <extension base="gdml:AbstractMaterialType">
                <sequence>
                    <element name="name" type="gdml:OrganismNameType" />
                </sequence>

                <attribute name="ncbiTaxID" type="gdml:NCBITaxidType" use="optional" />
            </extension>

        </complexContent>

    </complexType>



    <complexType name="OrganismNameType">
        <sequence>
            <element name="genus" type="gdml:UpperCaseNameType" />
            <element name="species" type="gdml:LowerCaseNameType" />
            <element name="strain" type="token" />
            <element name="typeStrain">
                <simpleType>
                    <restriction base="boolean">
                        <pattern value="true|false" />
                    </restriction>
                </simpleType>
            </element>
        </sequence>
    </complexType>


    <element name="_SampleLocation" abstract="true">
        <annotation>
            <documentation xml:lang="en">Global element which acts as head of the substitutionGroup
                that may include all types of sample locations</documentation>
        </annotation>
    </element>


    <element name="samplePointLocation" type="gdml:SamplePointLocationType"
        substitutionGroup="gdml:_SampleLocation" />

    <complexType name="SamplePointLocationType">
        <annotation>
            <documentation>Give the location of a sample in a simplified way.</documentation>
        </annotation>
        <sequence>
            <element ref="gml:LocationKeyWord" minOccurs="0" />
            <element ref="gml:LocationString" minOccurs="0" />
            <element name="pos2D">
                <complexType>
                    <simpleContent>
                        <restriction base="gml:DirectPositionType">
                            <length value="2" />
                        </restriction>
                    </simpleContent>
                </complexType>
            </element>
            <!-- TODO make this as an attribue-->
            <element name="determinationMethod" type="string" />
        </sequence>
    </complexType>

    <element name="sampleLocation" type="gdml:SampleLocationType"
        substitutionGroup="gdml:_SampleLocation" />

    <complexType name="SampleLocationType">
        <complexContent>
            <restriction base="gml:AbstractFeatureType">
                <annotation>
                    <documentation>Derived as suggested by GML 3.1.1 clause 8.2.6</documentation>
                </annotation>
                <sequence>
                    <element ref="gml:metaDataProperty" minOccurs="0" maxOccurs="0" />
                    <element ref="gml:description" minOccurs="0" />
                    <element ref="gml:name" minOccurs="1" maxOccurs="unbounded" />
                    <element ref="gml:boundedBy" minOccurs="0" />
                    <element ref="gml:location" minOccurs="1" />
                </sequence>
            </restriction>
        </complexContent>
    </complexType>


    <!-- ============================================================== -->
    <!-- == RSBI related stuff ================================================= -->
    <!-- ============================================================== -->
    <element name="_Material" abstract="true">
        <annotation>
            <documentation xml:lang="en">Global element which acts as head of the substitutionGroup
                that may include all types of sample and biomaterial</documentation>
        </annotation>
    </element>

    <element name="study" abstract="true">
        <annotation>
            <documentation>Fom RSBI: "A Study is an experiment with distinct periods or Phase(s),
                having experimental procedure, or Action(s), applied to Subject(s) or Group(s) of
                these."</documentation>
            <documentation xml:lang="en">Global element which acts as head of the substitutionGroup
                that may include all types reports and summarys of conducted or ongoing
            studies.</documentation>
        </annotation>
    </element>

    <!-- ============================================================== -->
    <!-- == DNA Library Types ================================================= -->
    <!-- ============================================================== -->


    <!--<element name="DnaLibrary" type="gdml:AbstractDnaLibraryType" substitutionGroup="gdml:_Material"
        abstract="true" />
-->

    <complexType name="AbstractDnaLibraryType" abstract="true">
        <sequence>
            <element ref="gdml:publication" minOccurs="0" maxOccurs="unbounded" />
            <element ref="gdml:librarySize" minOccurs="0" />
            <element ref="gdml:numSequencedClones" minOccurs="0" />
            <element ref="gdml:cloneVector" minOccurs="0" />
        </sequence>
        <attribute name="label" type="string" use="optional" />
    </complexType>

    <complexType name="GenomeDNALibraryType">
        <complexContent>
            <restriction base="gdml:AbstractDnaLibraryType">
                <sequence>
                    <element ref="gdml:publication" minOccurs="0" maxOccurs="unbounded" />
                    <element ref="gdml:librarySize" minOccurs="0" />
                    <element ref="gdml:numSequencedClones" minOccurs="0" />
                    <element ref="gdml:cloneVector" minOccurs="0" />
                </sequence>
                <attribute name="label" use="required" type="string" />
            </restriction>
        </complexContent>
    </complexType>

    <complexType name="MetagenomeLibraryType">
        <complexContent>
            <restriction base="gdml:AbstractDnaLibraryType">
                <sequence>
                    <element ref="gdml:publication" minOccurs="0" maxOccurs="unbounded" />
                    <element ref="gdml:librarySize" minOccurs="1" />
                    <element ref="gdml:numSequencedClones" minOccurs="1" />
                    <element ref="gdml:cloneVector" minOccurs="1" />
                </sequence>
            </restriction>
        </complexContent>
    </complexType>



    <element name="dnaFilter" type="gdml:DnaFilterType" substitutionGroup="gdml:_Material" />

    <complexType name="DnaFilterType">
        <sequence>
            <element name="name" type="string" />
            <element name="poreSize" type="gdml:MeasureType" />
        </sequence>
    </complexType>


    <element name="dnaExtract" substitutionGroup="gdml:_Material" />

    <!-- ============================================================== -->
    <!-- == Isolate Types ================================================= -->
    <!-- ============================================================== -->

    <complexType name="AbstractIsolateType" abstract="true">
        <sequence>
            <element ref="gdml:publication" minOccurs="0" maxOccurs="unbounded" />
            <element ref="gdml:cultureCollection" maxOccurs="unbounded" />
        </sequence>
    </complexType>

    <!-- ============================================================== -->
    <!-- == Organelle Isolate Types ============================================-->
    <!-- ============================================================== -->

    <complexType name="OrganelleIsolateType">
        <complexContent>
            <extension base="gdml:AbstractIsolateType">
                <sequence>
                    <element ref="gdml:estimatedSize" minOccurs="0" />

                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <!-- ============================================================== -->
    <!-- == Eukaryote Isolate Types ========================================== -->
    <!-- ============================================================== -->

    <complexType name="EukaryoticIsolateType">
        <complexContent>
            <extension base="gdml:AbstractIsolateType">
                <sequence>
                    <element ref="gdml:trophicLevel" />
                    <element ref="gdml:ploidyLevel" />
                    <element ref="gdml:numReplicons" />
                    <element ref="gdml:extrachromosomalElements" minOccurs="0" />
                    <element ref="gdml:estimatedSize" />
                    <element ref="gdml:originalHost" minOccurs="0" />
                    <element ref="gdml:hostSpecificityAndRange" minOccurs="0" />
                    <element ref="gdml:pathogenicity" minOccurs="0" />
                    <element ref="gdml:propagation" />
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <!-- ============================================================== -->
    <!-- == Prokaryotic isolate Types ========================================= -->
    <!-- ============================================================== -->

    <complexType name="ProkaryoticIsolateType">
        <complexContent>
            <extension base="gdml:AbstractIsolateType">
                <sequence>
                    <element ref="gdml:trophicLevel" />
                    <element ref="gdml:numReplicons" />
                    <element ref="gdml:bioticRelationship" />
                    <element ref="gdml:oxygenRelation" />
                    <element ref="gdml:ploidyLevel" minOccurs="0" />
                    <element ref="gdml:extrachromosomalElements" />
                    <element ref="gdml:estimatedSize" minOccurs="0" />
                    <element ref="gdml:originalHost" />
                    <element ref="gdml:hostSpecificityAndRange" minOccurs="0" />
                    <element ref="gdml:healthDiseaseStatus" />
                    <element ref="gdml:pathogenicity" minOccurs="0" />
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <!-- ============================================================== -->
    <!-- == Plasmid isolate Types ============================================ -->
    <!-- ============================================================== -->

    <complexType name="PlasmidIsolateType">
        <complexContent>
            <extension base="gdml:AbstractIsolateType">
                <sequence>
                    <element ref="gdml:bioticRelationship" />
                    <element ref="gdml:estimatedSize" minOccurs="0" />
                    <element ref="gdml:originalHost" />
                    <element ref="gdml:hostSpecificityAndRange" minOccurs="0" />
                    <element ref="gdml:propagation" />
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <!-- ============================================================== -->
    <!-- == Virus Isolate Types ================================================ -->
    <!-- ============================================================== -->

    <complexType name="VirusIsolateType">
        <complexContent>
            <extension base="gdml:AbstractIsolateType">
                <sequence>
                    <element ref="gdml:numReplicons" />
                    <element ref="gdml:ploidyLevel" minOccurs="0" />
                    <element ref="gdml:estimatedSize" minOccurs="0" />
                    <element ref="gdml:bioticRelationship" />
                    <element ref="gdml:originalHost" />
                    <element ref="gdml:alternateHost" minOccurs="0" maxOccurs="unbounded" />
                    <element ref="gdml:healthDiseaseStatus" />
                    <element ref="gdml:pathogenicity" />
                    <element ref="gdml:propagation" />
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <!--
    <complexType name="growthConditions">
        <sequence>
            <element ref="gdml:isolationGrowthConditions" />
        </sequence>
    </complexType>-->


    <!-- ============================================================== -->
    <!-- == Sequencing Types ============================================= -->
    <!-- ============================================================== -->

    <!--<complexType name="AbstractSequencingType" abstract="true">
        <sequence>
             Mandatory for all kinds of GSC Reports
            <element ref="gdml:sequencingMethod" />
            Mandatory for all kinds of GSC Reports
            <element ref="gdml:assembly" />
        </sequence>
    </complexType>
-->

    <simpleType name="GcContentType">
        <restriction base="decimal">
            <fractionDigits value="2" />
            <totalDigits value="5" />
            <maxInclusive value="100" />
            <minInclusive value="0" />
        </restriction>
    </simpleType>
    <!--
    <complexType name="GenomeSequencingOLD">
        <annotation>
            <documentation>Information on the completeness of the genome project (e.g. whether draft
                or closed, fold coverage and the final number of contigs).</documentation>
        </annotation>
        <complexContent>
            <extension base="gdml:AbstractSequencingType">
                <sequence>
                    <element ref="gdml:finishingStrategy" />
                    <element ref="gdml:foldCoverage" />
                    <element ref="gdml:numContigs" />
                </sequence>
            </extension>
        </complexContent>
    </complexType>
-->

    <element name="from-library">
        <annotation>
            <documentation>The DNA library the sequence is obtained from.</documentation>
        </annotation>
        <complexType>
            <attribute name="ref" type="string" use="required" />
        </complexType>
    </element>


    <complexType name="FullSequenceType">
        <sequence>
            <element name="accession" type="gdml:AccessionNumberType">
                <annotation>
                    <documentation>The accession number as given by members of
                    INSDC.</documentation>
                </annotation>
            </element>
            <element ref="gdml:assembly" />
            <element ref="gdml:from-library" minOccurs="0" maxOccurs="unbounded" />
        </sequence>
        <attribute name="label" use="required" type="string">
            <annotation>
                <documentation>Arbitray label. Required in order to distinguish more than one
                    chromosome of a genome.</documentation>
            </annotation>
        </attribute>
        <attribute name="size" use="optional" type="positiveInteger" />
        <attribute name="gc" use="optional" type="gdml:GcContentType" />
        <attribute name="coverage" type="gdml:foldCoverageType" />
        <attribute ref="gdml:sequencingMethod" use="optional" />
    </complexType>


    <complexType name="GenomeSequencingType">
        <choice>
            <element name="complete">
                <complexType>
                    <sequence>
                        <element name="chromosome" type="gdml:FullSequenceType">
                            <annotation>
                                <documentation>The completly finished chromsome of an
                                organism.</documentation>
                            </annotation>
                        </element>
                        <element name="plasmid" type="gdml:FullSequenceType" minOccurs="0"
                            maxOccurs="unbounded">
                            <annotation>
                                <documentation>The completly finished plasmid of an
                                organism.</documentation>
                            </annotation>
                        </element>
                    </sequence>
                    <attribute name="overallSequencingMethod" type="gdml:sequencingMethodEnumType"
                        use="required" />
                </complexType>
            </element>

            <element name="draft">
                <complexType>
                    <sequence>
                        <element name="scaffoldSet" type="gdml:scaffoldSetType" minOccurs="0" />
                        <element name="contigSet" type="gdml:contigSetType" minOccurs="0" />
                        <element name="readSet" type="gdml:readSetType" minOccurs="0" />
                    </sequence>
                    <attribute name="finishingStrategy" type="gdml:finishingStrategyType" />
                    <attribute name="overallSequencingMethod" type="gdml:sequencingMethodEnumType"
                        use="required" />
                </complexType>
            </element>
        </choice>
    </complexType>

    <complexType name="EukaryoticGenomeSequencingType">
        <choice>
            <element name="complete">
                <complexType>
                    <sequence>
                        <element name="chromosome" type="gdml:FullSequenceType">
                            <annotation>
                                <documentation>The completly finished chromsome of an
                                organism.</documentation>
                            </annotation>
                        </element>

                    </sequence>
                    <attribute name="overallSequencingMethod" type="gdml:sequencingMethodEnumType"
                        use="required" />
                </complexType>
            </element>

            <element name="draft">
                <complexType>
                    <sequence>
                        <element name="scaffoldSet" type="gdml:scaffoldSetType" minOccurs="0" />
                        <element name="contigSet" type="gdml:contigSetType" minOccurs="0" />
                        <element name="readSet" type="gdml:readSetType" minOccurs="0" />
                    </sequence>
                    <attribute name="finishingStrategy" type="gdml:finishingStrategyType" />
                    <attribute name="overallSequencingMethod" type="gdml:sequencingMethodEnumType"
                        use="required" />
                </complexType>
            </element>
        </choice>
    </complexType>

    <complexType name="PlasmidGenomeSequencingType">
        <choice>
            <element name="complete">
                <complexType>
                    <sequence>
                        <element name="plasmid" type="gdml:FullSequenceType" minOccurs="0">
                            <annotation>
                                <documentation>The completly finished plasmid of an
                                organism.</documentation>
                            </annotation>
                        </element>
                    </sequence>
                    <attribute name="overallSequencingMethod" type="gdml:sequencingMethodEnumType"
                        use="required" />
                </complexType>
            </element>

            <element name="draft">
                <complexType>
                    <sequence>
                        <element name="scaffoldSet" type="gdml:scaffoldSetType" minOccurs="0" />
                        <element name="contigSet" type="gdml:contigSetType" minOccurs="0" />
                        <element name="readSet" type="gdml:readSetType" minOccurs="0" />
                    </sequence>
                    <attribute name="finishingStrategy" type="gdml:finishingStrategyType" />
                    <attribute name="overallSequencingMethod" type="gdml:sequencingMethodEnumType"
                        use="required" />
                </complexType>
            </element>
        </choice>
    </complexType>

    <complexType name="VirusGenomeSequencingType">
        <choice>
            <element name="complete">
                <complexType>
                    <sequence>
                        <element name="segment" type="gdml:FullSequenceType">
                            <annotation>
                                <documentation>The completly finished chromsome of an
                                    organism.</documentation>
                            </annotation>
                        </element>
                        
                    </sequence>
                    <attribute name="overallSequencingMethod" type="gdml:sequencingMethodEnumType"
                        use="required" />
                </complexType>
            </element>
            
            <element name="draft">
                <complexType>
                    <sequence>
                        <element name="scaffoldSet" type="gdml:scaffoldSetType" minOccurs="0" />
                        <element name="contigSet" type="gdml:contigSetType" minOccurs="0" />
                        <element name="readSet" type="gdml:readSetType" minOccurs="0" />
                    </sequence>
                    <attribute name="finishingStrategy" type="gdml:finishingStrategyType" />
                    <attribute name="overallSequencingMethod" type="gdml:sequencingMethodEnumType"
                        use="required" />
                </complexType>
            </element>
        </choice>
    </complexType>
    

    <element name="idList">
        <annotation>
            <documentation>List of sequence ids.</documentation>
        </annotation>
        <simpleType>
            <list itemType="string" />
        </simpleType>
    </element>

    <element name="idRange">
        <annotation>
            <documentation>Range of sequence ids.</documentation>
        </annotation>
        <simpleType>
            <restriction base="string">
                <pattern value=".*-.*" />
                <minLength value="3" />
            </restriction>
        </simpleType>
    </element>

    <group name="rangeORlist">
        <choice>
            <element ref="gdml:idRange" />
            <element ref="gdml:idList" />
        </choice>
    </group>

    <attribute name="label" type="string" />
    <attribute name="sequencingMethod" type="gdml:sequencingMethodEnumType">
        <annotation>
            <documentation>Optional description of sequencing method.</documentation>
        </annotation>
    </attribute>

    <complexType name="scaffoldSetType">
        <sequence>
            <group ref="gdml:rangeORlist" />
            <element ref="gdml:assembly" minOccurs="0" />
            <element ref="gdml:from-library" minOccurs="0" maxOccurs="unbounded" />
        </sequence>
        <attribute ref="gdml:label" use="optional" />
        <attribute ref="gdml:sequencingMethod" use="optional" />
    </complexType>
    <!-- TODO: combine scaffold and contigsets -->
    <complexType name="contigSetType">
        <sequence>
            <group ref="gdml:rangeORlist" />
            <element ref="gdml:assembly" minOccurs="0" />
            <element ref="gdml:from-library" minOccurs="0" maxOccurs="unbounded" />
        </sequence>
        <attribute ref="gdml:label" use="optional" />
        <attribute ref="gdml:sequencingMethod" use="optional" />
    </complexType>


    <complexType name="readSetType">
        <sequence>
            <group ref="gdml:rangeORlist" />
            <element ref="gdml:from-library" minOccurs="0" />
        </sequence>
        <attribute ref="gdml:label" use="optional" />
        <attribute ref="gdml:sequencingMethod" use="optional" />
    </complexType>

    <!--   <complexType name="MetagenomeSequencingType">
        <complexContent>
            <extension base="gdml:AbstractSequencingType">
                <sequence>
                    <element ref="gdml:fragmentClassificationMethod" />
                    <element ref="gdml:foldCoverage" minOccurs="0" />
                    <element ref="gdml:numContigs" minOccurs="0" />
                </sequence>
            </extension>
        </complexContent>
    </complexType>-->

    <complexType name="MetagenomeSequencingType">
        <sequence>
            <element ref="gdml:fragmentClassificationMethod" />
            <element ref="gdml:foldCoverage" minOccurs="0" />
            <element ref="gdml:numContigs" minOccurs="0" />
            <element name="scaffoldSet" type="gdml:scaffoldSetType" />
            <element name="contigSet" type="gdml:contigSetType" />
            <element name="readSet" type="gdml:readSetType" />
        </sequence>
    </complexType>



    <group name="externalIdentifiers">
        <sequence>
            <element ref="gdml:gcatID" minOccurs="0" />
            <element ref="gdml:goldStamp" minOccurs="0" />
            <element ref="gdml:genomeProjectID" minOccurs="0" />
        </sequence>
    </group>


    <!-- ============================================================== -->
    <!-- == Minimal Reports ========================================= -->
    <!-- ============================================================== -->
    <element name="_NasReport" abstract="true" substitutionGroup="gdml:study">
        <annotation>
            <documentation xml:lang="en">Global element which acts as head of the substitutionGroup
                that may include all types of reports</documentation>
            <documentation>A report is a defined structure that determines what and how results of a
                finished study should be reported</documentation>
        </annotation>
    </element>


    <!-- ============================================================== -->
    <!-- == Minimal Eukaryotic Report ======================================== -->
    <!-- ============================================================== -->

    <element name="eukaryote" type="gdml:EukaryoteReportType" substitutionGroup="gdml:_NasReport">
        <annotation>
            <documentation>Report about eukaryotic genome sequences. MIGS/MIMS compliant
            </documentation>
        </annotation>
        <key name="eukaryoteLibrary">
            <selector xpath="gdml:dnaLibrary" />
            <field xpath="@label" />
        </key>

        <keyref refer="gdml:eukaryoteLibrary" name="eukaryote-complete-lib-ref">
            <selector xpath="gdml:sequencing/gdml:complete/gdml:chromosome/gdml:from-library" />
            <field xpath="@ref" />
        </keyref>
        <keyref refer="gdml:eukaryoteLibrary" name="eukaryote-draft-lib-ref">
            <selector
                xpath="gdml:sequencing/gdml:draft/gdml:scaffoldSet/gdml:from-library|gdml:sequencing/gdml:draft/gdml:contigSet/gdml:from-library|gdml:sequencing/gdml:draft/gdml:readSet/gdml:from-library" />
            <field xpath="@ref" />
        </keyref>
    </element>

    <complexType name="EukaryoteReportType">
        <sequence>
            <element name="ncbiOrganismName" type="gdml:GenomeProjectOrganismNameType" />
            <element name="ncbiTaxID" type="gdml:NCBITaxidType" minOccurs="0" />

            <group ref="gdml:externalIdentifiers" />

            <element ref="gdml:originalSample" />

            <element name="Isolate" type="gdml:EukaryoticIsolateType" minOccurs="1">
                <annotation>
                    <documentation>In case of unicelluar microorgnism</documentation>
                </annotation>
            </element>

            <element ref="gdml:nucExtract" />

            <element name="dnaLibrary" type="gdml:GenomeDNALibraryType" minOccurs="0">
                <annotation>
                    <documentation>Description of the libraries used for for
                    sequencing</documentation>
                </annotation>
            </element>

            <element name="sequencing" type="gdml:EukaryoticGenomeSequencingType" />

        </sequence>
    </complexType>

    <!--    Not tested yet therefore no specific type-->

    <!-- ============================================================== -->
    <!-- == Minimal Prokaryotic Report ========================================= -->
    <!-- ============================================================== -->

    <element name="prokaryote" type="gdml:ProkaroyteReportType" substitutionGroup="gdml:_NasReport">

        <key name="prokaryoteLibrary">
            <selector xpath="gdml:dnaLibrary" />
            <field xpath="@label" />
        </key>

        <keyref refer="gdml:prokaryoteLibrary" name="prokaryote-complete-lib-ref">
            <selector
                xpath="gdml:sequencing/gdml:complete/gdml:chromosome/gdml:from-library|gdml:sequencing/gdml:complete/gdml:plasmid/gdml:from-library" />
            <field xpath="@ref" />
        </keyref>
        <keyref refer="gdml:prokaryoteLibrary" name="prokaryote-draft-lib-ref">
            <selector
                xpath="gdml:sequencing/gdml:draft/gdml:scaffoldSet/gdml:from-library|gdml:sequencing/gdml:draft/gdml:contigSet/gdml:from-library|gdml:sequencing/gdml:draft/gdml:readSet/gdml:from-library" />
            <field xpath="@ref" />
        </keyref>
    </element>

    <complexType name="ProkaroyteReportType">

        <sequence>
            <element name="ncbiOrganismName" type="gdml:GenomeProjectOrganismNameType" />
            <element name="ncbiTaxID" type="gdml:NCBITaxidType" minOccurs="0" />

            <group ref="gdml:externalIdentifiers" />

            <element ref="gdml:originalSample" />

            <element name="Isolate" type="gdml:ProkaryoticIsolateType" minOccurs="1">
                <annotation>
                    <documentation>In case of unicelluar microorgnism</documentation>
                </annotation>
            </element>

            <element ref="gdml:nucExtract" />

            <element name="dnaLibrary" type="gdml:GenomeDNALibraryType" minOccurs="0" />

            <element name="sequencing" type="gdml:GenomeSequencingType" />
        </sequence>
    </complexType>





    <!-- ============================================================== -->
    <!-- == Minimal Plasmid Report =========================================== -->
    <!-- ============================================================== -->

    <element name="plasmid" type="gdml:PlasmidReportType" substitutionGroup="gdml:_NasReport">

        <key name="plasmidLibrary">
            <selector xpath="gdml:dnaLibrary" />
            <field xpath="@label" />
        </key>

        <keyref refer="gdml:plasmidLibrary" name="plasmid-complete-lib-ref">
            <selector xpath="gdml:sequencing/gdml:complete/gdml:plasmid/gdml:from-library" />
            <field xpath="@ref" />
        </keyref>
        <keyref refer="gdml:plasmidLibrary" name="plasmid-draft-lib-ref">
            <selector
                xpath="gdml:sequencing/gdml:draft/gdml:scaffoldSet/gdml:from-library|gdml:sequencing/gdml:draft/gdml:contigSet/gdml:from-library|gdml:sequencing/gdml:draft/gdml:readSet/gdml:from-library" />
            <field xpath="@ref" />
        </keyref>
    </element>

    <complexType name="PlasmidReportType">
        <sequence>
            <element name="ncbiOrganismName" type="gdml:GenomeProjectOrganismNameType" />
            <element name="ncbiTaxID" type="gdml:NCBITaxidType" minOccurs="0" />

            <group ref="gdml:externalIdentifiers" />

            <element ref="gdml:originalSample" />

            <element name="Isolate" type="gdml:PlasmidIsolateType" minOccurs="1">
                <annotation>
                    <documentation>In case of unicelluar microorgnism</documentation>
                </annotation>
            </element>

            <element ref="gdml:nucExtract" />

            <element name="dnaLibrary" type="gdml:GenomeDNALibraryType" minOccurs="0" />

            <element name="sequencing" type="gdml:PlasmidGenomeSequencingType" minOccurs="1" />

        </sequence>

    </complexType>

    <!-- ============================================================== -->
    <!-- == Minimal Virus Report =========================================== -->
    <!-- ============================================================== -->

    <element name="virus" type="gdml:VirusReportType" substitutionGroup="gdml:_NasReport" >
        <key name="virusLibrary">
            <selector xpath="gdml:dnaLibrary" />
            <field xpath="@label" />
        </key>
        
        <keyref refer="gdml:virusLibrary" name="virus-complete-lib-ref">
            <selector xpath="gdml:sequencing/gdml:complete/gdml:segment/gdml:from-library" />
            <field xpath="@ref" />
        </keyref>
        <keyref refer="gdml:virusLibrary" name="virus-draft-lib-ref">
            <selector
                xpath="gdml:sequencing/gdml:draft/gdml:scaffoldSet/gdml:from-library|gdml:sequencing/gdml:draft/gdml:contigSet/gdml:from-library|gdml:sequencing/gdml:draft/gdml:readSet/gdml:from-library" />
            <field xpath="@ref" />
        </keyref>
    </element>

    <complexType name="VirusReportType">
        <sequence>
            <element name="ncbiOrganismName" type="gdml:GenomeProjectOrganismNameType" />
            <element name="ncbiTaxID" type="gdml:NCBITaxidType" minOccurs="0" />

            <group ref="gdml:externalIdentifiers" />

            <element ref="gdml:originalSample" />

            <element name="isolate" type="gdml:VirusIsolateType" minOccurs="1" />

            <element ref="gdml:nucExtract" />

            <element name="dnaLibrary" type="gdml:GenomeDNALibraryType" minOccurs="0" />

            <element name="sequencing" type="gdml:VirusGenomeSequencingType" minOccurs="1" />

        </sequence>

    </complexType>

    <!-- ============================================================== -->
    <!-- == Minimal Metagenome Report ======================================== -->
    <!-- ============================================================== -->

    <element name="metagenome" type="gdml:MetagenomeReportType" substitutionGroup="gdml:_NasReport" >
        <key name="metagenomeLibrary">
            <selector xpath="gdml:dnaLibrary" />
            <field xpath="@label" />
        </key>
        
        <keyref refer="gdml:metagenomeLibrary" name="metagenome-lib-ref">
            <selector
                xpath="gdml:sequencing/gdml:scaffoldSet/gdml:from-library|gdml:sequencing/gdml:contigSet/gdml:from-library|gdml:sequencing/gdml:readSet/gdml:from-library" />
            <field xpath="@ref" />
        </keyref>
        
    </element>

    <complexType name="MetagenomeReportType">
        <sequence>

            <group ref="gdml:externalIdentifiers" />

            <element name="originalSample" type="gdml:SourceSampleMetagenomicType">
                <annotation>
                    <documentation xml:lang="en">The environmental source description of the
                        original, physical material or organismal sample collected from the
                        environment. </documentation>
                </annotation>

            </element>

            <element name="Isolate" type="gdml:EukaryoticIsolateType" minOccurs="0">
                <annotation>
                    <documentation>In case of unicelluar microorgnism</documentation>
                </annotation>
            </element>

            <element ref="gdml:nucExtract" />

            <element name="dnaLibrary" type="gdml:MetagenomeLibraryType" minOccurs="1">
                <annotation>
                    <documentation>Description of the libraries used for for
                    sequencing</documentation>
                </annotation>
            </element>

            <element name="sequencing" type="gdml:MetagenomeSequencingType" minOccurs="1" />

        </sequence>
    </complexType>

    <!-- ============================================================== -->
    <!-- == Minimal Organelle Report =========================================== -->
    <!-- ============================================================== -->

    <element name="organelle" type="gdml:OrganelleReportType" substitutionGroup="gdml:_NasReport" >
        <key name="organelleLibrary">
            <selector xpath="gdml:dnaLibrary" />
            <field xpath="@label" />
        </key>
        
        <keyref refer="gdml:organelleLibrary" name="organelle-complete-lib-ref">
            <selector xpath="gdml:sequencing/gdml:complete/gdml:chromosome/gdml:from-library" />
            <field xpath="@ref" />
        </keyref>
        <keyref refer="gdml:organelleLibrary" name="organelle-draft-lib-ref">
            <selector
                xpath="gdml:sequencing/gdml:draft/gdml:scaffoldSet/gdml:from-library|gdml:sequencing/gdml:draft/gdml:contigSet/gdml:from-library|gdml:sequencing/gdml:draft/gdml:readSet/gdml:from-library" />
            <field xpath="@ref" />
        </keyref>
        
    </element>

    <complexType name="OrganelleReportType">
        <sequence>
            <element name="ncbiOrganismName" type="gdml:GenomeProjectOrganismNameType" />
            <element name="ncbiTaxID" type="gdml:NCBITaxidType" minOccurs="0" />

            <group ref="gdml:externalIdentifiers" />

            <element ref="gdml:originalSample" />

            <element name="isolate" type="gdml:OrganelleIsolateType" minOccurs="1">
                <annotation>
                    <documentation>In case of unicelluar microorgnism</documentation>
                </annotation>
            </element>

            <element ref="gdml:nucExtract" />

            <element name="dnaLibrary" type="gdml:GenomeDNALibraryType" minOccurs="0">
                <annotation>
                    <documentation>Description of the libraries used for for
                    sequencing</documentation>
                </annotation>
            </element>

            <element name="sequencing" type="gdml:EukaryoticGenomeSequencingType" minOccurs="1" />

        </sequence>
    </complexType>




    <!-- ============================================================== -->
    <!-- == Other Stuff  ==================================================== -->
    <!-- ============================================================== -->

    <simpleType name="ReportSimpleType">
        <annotation>
            <documentation>Enumeration of types of report.</documentation>
        </annotation>
        <restriction base="token">
            <enumeration value="Metagenome" />
            <enumeration value="Bacteria" />
            <enumeration value="Archaea" />
            <enumeration value="Plasmid" />
            <enumeration value="Eukaryote" />
            <enumeration value="Virus" />
            <enumeration value="Phage" />
            <enumeration value="Mitochondrion" />
            <enumeration value="Chloroplast" />
            <enumeration value="Nucleomorph" />
        </restriction>
    </simpleType>


    <element name="samplingStrategy">
        <complexType>
            <simpleContent>
                <extension base="gdml:samplingStrategyEnumType"> </extension>
            </simpleContent>
        </complexType>
    </element>

    <element name="isolationGrowthConditions">
        <annotation>
            <documentation>Information about the conditions required to grow the isolate (genome) in
                the laboratory; for a genetic element; for example information on suitable host.
                This is in addition to reference for biometerial and the original genome
                publication. Right now this is free text but should accept URLs, DOIs, PMIDs or SOPs
                and published information on this.</documentation>
        </annotation>
        <complexType>
            <simpleContent>
                <extension base="string"> </extension>
            </simpleContent>
        </complexType>
    </element>

    <element name="nucExtract">
        <annotation>
            <documentation>How was the genetic (DNA or RNA) material extracted and prepared for
                library construction (e.g. MDA, emPCR, plones) Input: CV</documentation>
        </annotation>
        <complexType>
            <sequence>
                <element name="method" type="gdml:nucExtractionEnumType" />
                <element ref="gdml:publication" minOccurs="0" />
            </sequence>
        </complexType>
    </element>


    <element name="dnaAmplificationMethod">
        <annotation>
            <documentation>How was the genetic material amplified for library construction (e.g.
                MDA, emPCR, plones)</documentation>
        </annotation>
        <complexType>
            <simpleContent>
                <extension base="gdml:dnaAmplificationEnumType"> </extension>
            </simpleContent>
        </complexType>
    </element>



    <element name="numSequencedClones" type="gdml:posIntegerOrNull">
        <annotation>
            <documentation>Total number of clones sequenced for this project. Input:
            integer</documentation>
        </annotation>
    </element>

    <element name="sequencingMethod">
        <annotation>
            <documentation>The name of the sequencing method used (e.g. dideoxysequencing,
                pryosequencing, polony, synthesis with protected dNTPs). Input: CV</documentation>
        </annotation>
        <complexType>
            <simpleContent>
                <extension base="gdml:sequencingMethodEnumType"> </extension>
            </simpleContent>
        </complexType>
    </element>

    <element name="estimatedErrorRate" type="gdml:posIntegerOrNull">
        <annotation>
            <documentation>Estimated error rate associated with the finished sequences and method of
                calculation (e.g. error rate of 1 in 1000 bp). </documentation>
        </annotation>
    </element>

    <element name="assemblyMethod" type="gdml:assemblyMethodEnumType">
        <annotation>
            <documentation>How was the assembly done (e.g. with a text based assembler like phrap or
                a flowgram assembler etc). Input: CV</documentation>
        </annotation>
    </element>

    <!-- TODO method of calculation is missing -->
    <element name="assembly">
        <complexType>
            <sequence>
                <element ref="gdml:assemblyMethod" />
                <element ref="gdml:estimatedErrorRate" />
            </sequence>
        </complexType>
    </element>

    <element name="numContigs">
        <annotation>
            <documentation>How many contigs were produced for the genome. integer.</documentation>
        </annotation>
        <complexType>
            <simpleContent>
                <extension base="positiveInteger" />
            </simpleContent>
        </complexType>
    </element>


    <element name="organism">
        <annotation>
            <documentation>The information in organism describes the biological origin of each
                genome or metagenomic and provides details on how to obtain it (e.g. the isolate
                from a culture collection). Everything should be specific</documentation>
        </annotation>

        <complexType>
            <sequence>
                <element ref="gdml:geneticLineage" />
                <element ref="gdml:ploidyLevel" />
                <element ref="gdml:numReplicons" />
                <element ref="gdml:extrachromosomalElements" />
                <element ref="gdml:estimatedSize" />
                <!--                <element ref="gdml:biomaterialReference" />-->
                <element ref="gdml:cultureCollection" />
                <element ref="gdml:bioticRelationship" />
                <element ref="gdml:originalHost" />
                <element ref="gdml:hostSpecificityAndRange" />

                <element ref="gdml:healthDiseaseStatus" />

                <element ref="gdml:pathogenicity" />
                <element ref="gdml:trophicLevel" />
                <element ref="gdml:estimatedCommunityDiversity" />

            </sequence>
        </complexType>
    </element>

    <!-- ======================================== -->
    <!-- == Elements from older MIGS/MIMS Schemea ============ -->
    <!-- ============================================================ -->
    <element name="_Assay" abstract="true" />

    <complexType name="InvestigationType">
        <annotation>
            <documentation>Investigation as defined by RSBI and supported by MICheck
                http://mibbi.sourceforge.net/. An investigation in MIGS refers to the general of a
                single genome or metagenomic dataset.</documentation>
        </annotation>

        <sequence>
            <element name="study" type="gdml:StudyType" />

            <element name="Assay">
                <annotation>
                    <documentation>Assay as defined by RSBI and supported by MICheck
                        http://mibbi.sourceforge.net/</documentation>
                </annotation>
                <complexType>
                    <sequence>

                        <element name="sample_processing">
                            <annotation>
                                <documentation>The information in sample processing describes the
                                    way a biological sample was processing after collection and
                                    sequenced.</documentation>
                            </annotation>
                            <complexType>
                                <sequence>
                                    <element ref="gdml:isolationGrowthConditions" />
                                    <element ref="gdml:nucExtract" />
                                    <element ref="gdml:dnaAmplificationMethod" />
                                    <!--                                    <element ref="gdml:DnaLibrary" />-->
                                    <element ref="gdml:sequencingMethod" />
                                </sequence>
                            </complexType>
                        </element>

                        <!--                        <element ref="gdml:dataProcessing" />-->

                    </sequence>
                </complexType>
            </element>
        </sequence>
    </complexType>


    <element name="phenotype">
        <annotation>
            <documentation>The information in phenotype describes information about physical
                characteristics and other traits.</documentation>
        </annotation>
        <complexType>
            <sequence>
                <element ref="gdml:propagation" />
                <element ref="gdml:encodedTraits" />
                <element ref="gdml:oxygenRelation" />
            </sequence>
        </complexType>
    </element>

    <complexType name="StudyType">
        <annotation>
            <documentation>Study as defined by RSBI and supported by MICheck
                http://mibbi.sourceforge.net/</documentation>
        </annotation>

        <sequence>
            <element ref="gdml:organism" />
            <element ref="gdml:phenotype" />
        </sequence>
    </complexType>


</schema>
