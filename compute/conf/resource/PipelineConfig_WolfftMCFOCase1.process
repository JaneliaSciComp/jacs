<process name="Tanya Central Brain MCFO Case 1 Pipeline" processor="queue/samplePipelineLauncher" startEvent="pending" updateProcessStatus="on_success">
    <sequence>
    
        <!-- Tanya asks for only reference channel in her 20x images -->
            
        <include name="Pipeline" process="FlyLightPairedSamplePipeline">
            <input name="PIPELINE_PROCESS" value="WolfftMCFOCase1"/>
            <input name="20X_CHANNEL_DYE_SPEC" value="reference=Alexa Fluor 488,Cy2;membrane_ha=,Alexa Fluor 633,Alexa Fluor 647,Cy5;membrane_v5=Alexa Fluor 546,Alexa Fluor 555,Alexa Fluor 568,DY-547;membrane_flag=Alexa Fluor 594"/>
            <input name="20X_OUTPUT_CHANNEL_ORDER" value="membrane_ha,membrane_v5,membrane_flag,reference"/>
            <input name="20X_OUTPUT_COLOR_SPEC" value="RGB1"/>
            <input name="63X_CHANNEL_DYE_SPEC" value="reference=Alexa Fluor 488,Cy2;membrane_ha=,Alexa Fluor 633,Alexa Fluor 647,Cy5;membrane_v5=Alexa Fluor 546,Alexa Fluor 555,Alexa Fluor 568,DY-547;membrane_flag=Alexa Fluor 594"/>
            <input name="63X_OUTPUT_CHANNEL_ORDER" value="membrane_ha,membrane_v5,membrane_flag,reference"/>
            <input name="63X_OUTPUT_COLOR_SPEC" value="RGB1"/>
            <input name="IMAGE_TYPE" value="mcfo"/>
        </include>
        
    </sequence>
</process>