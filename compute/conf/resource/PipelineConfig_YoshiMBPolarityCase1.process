<process name="Yoshi Mushroom Body Polarity Case 1 Pipeline" processor="queue/samplePipelineLauncher" startEvent="pending">
	<sequence>
    
        <!-- Set task variables -->
        <operation name="Import Variables" processor="org.janelia.it.jacs.compute.service.common.InitTaskVariablesService">
            <input name="TASK_PARAMETER_1" value="pipeline process"/>
            <input name="TASK_PARAMETER_VALUE_1" value="YoshiMBPolarityCase1"/>
        </operation>
        
	    <!-- Set task variables -->
	    <operation name="Init Variables" processor="org.janelia.it.jacs.compute.service.common.InitVariablesService">
	        <input name="PROCESS_VARIABLE_1" value="channel order"/>
            <input name="PROCESS_VARIABLE_VALUE_1" value=""/>
	    </operation>
	
        <include name="Pipeline" process="FlyLightPolarityPipeline"/>
		
    </sequence>
</process>