<process name="FlyLight Optic Lobe Pipeline" processor="queue/samplePipelineLauncher" startEvent="pending">
	<sequence>
	    
        <include name="Pipeline" process="FlyLightSamplePipeline">
            <input name="PIPELINE_NAME" value="FlyLight Pipeline"/>
            <input name="MERGE_ALGORITHMS" value="FLYLIGHT"/>
            <input name="STITCH_ALGORITHMS" value="FLYLIGHT"/>
            <input name="ALIGNMENT_ALGORITHMS" value="CONFIGURED"/>
            <input name="ALIGNMENT_ALGORITHM_PARAMS" value="rightopticlobealign_512px_INT.sh"/>
            <input name="ANALYSIS_ALGORITHMS" value="NEURON_SEPARATOR"/>
            <input name="PIPELINE_PROCESS" value="NernaRightOpticLobe"/>
        </include>
        
    </sequence>
</process>