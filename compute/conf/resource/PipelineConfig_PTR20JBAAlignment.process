<process name="PTR 20 JBA Alignment" processor="java:/jms/queue/samplePipelineLauncher" startEvent="pending" updateProcessStatus="on_success">
    <sequence>
    
        <include name="Pipeline" process="FlyLightPairedSamplePipeline">
            <input name="PIPELINE_PROCESS" value="FlyLightPairedSample"/>

            <!-- only perform JBA 20x alignment -->
            <input name="20X_ALIGNMENT_ALGORITHMS" value="CONFIGURED"/>
            <input name="20X_ALIGNMENT_ALGORITHM_PARAMS" value="flyalign20x_JBA_Qiscore.sh"/>
            <input name="20X_ALIGNMENT_ALGORITHM_RESULT_NAMES" value="JBA Alignment"/>

            <!-- no 20x neuron separation -->
            <input name="20X_ANALYSIS_ALGORITHMS" value=""/>

            <!-- no 63x alignment -->
            <input name="63X_ALIGNMENT_ALGORITHMS" value=""/>
            <input name="63X_ALIGNMENT_ALGORITHM_PARAMS" value=""/>
            <input name="63X_ALIGNMENT_ALGORITHM_RESULT_NAMES" value=""/>

            <!-- no 63x neuron separation -->
            <input name="63X_ANALYSIS_ALGORITHMS" value=""/>

        </include>

    </sequence>
</process>