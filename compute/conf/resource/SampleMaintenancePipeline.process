<!-- 
	Run all the routine Samples maintenance tasks for a given user. This includes cleaning out old un-annotated 
	results, compressing any stray V3DRAW files into PBD.
-->
<process name="Sample Maintenance Pipeline" processor="queue/largePipelineLauncher" startEvent="pending" updateProcessStatus="on_success">

	<sequence>

	    <include name="Sample Cleaning" process="SampleCleaning">
	    </include>
	    
        <include name="Remove Trash" process="SampleTrashCompactor">
        </include>
        
	    <include name="Compress Raw Files" process="SampleDataCompression">
	    </include>
        
    </sequence>
        
</process>