<!-- 
	Run all the routine Samples maintenance tasks for a given user. This includes cleaning out old un-annotated 
	results, compressing any stray V3DRAW files into PBD.
-->
<process name="Sample Maintenance Pipeline" processor="queue/mcfoDataPipelineLauncher" startEvent="pending" updateProcessStatus="on_success">

	<sequence>

	    <include name="Sample Cleaning" process="SampleCleaning">
	    </include>
	    
	    <include name="Compress raw files" process="MCFODataCompress">
	    </include>
    
    </sequence>
        
</process>