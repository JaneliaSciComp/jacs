<process name="Aljoscha MCFO Case 1 Pipeline Without 20x Merge" processor="queue/samplePipelineLauncher" startEvent="pending" updateProcessStatus="on_success">
    <sequence>

        <!--
          pre August 2013 20X samples are single lsms (no merge) with
               lsm-1:    dye-1: Alexa Fluor 488, dye-2: Alexa Fluor 647, dye-3: DY-547

          63X samples are paired lsms with
               lsm-1:    dye-1: Alexa Fluor 488, dye-2: Alexa Fluor 647, dye-3: DY-547
               lsm-2:    dye-1: Alexa Fluor 488, dye-2: Alexa Fluor 594
        -->

        <include name="Pipeline" process="FlyLightPairedSamplePipeline">
            <input name="PIPELINE_PROCESS" value="NernaMCFOCase1Without20xMerge"/>
            <input name="20X_CHANNEL_DYE_SPEC" value="membrane_ha=Alexa Fluor 633,Alexa Fluor 647,Cy5;membrane_v5=DY-547,Alexa Fluor 568;reference=Alexa Fluor 488,Cy2"/>
            <input name="20X_OUTPUT_CHANNEL_ORDER" value="membrane_ha,membrane_v5,reference"/>
            <input name="20X_OUTPUT_COLOR_SPEC" value="RGB1"/>
            <input name="63X_CHANNEL_DYE_SPEC" value="membrane_ha=Alexa Fluor 633,Alexa Fluor 647,Cy5;membrane_v5=DY-547,Alexa Fluor 568;membrane_flag=Alexa Fluor 594;reference=Alexa Fluor 488,Cy2"/>
            <input name="63X_OUTPUT_CHANNEL_ORDER" value="membrane_ha,membrane_v5,membrane_flag,reference"/>
            <input name="63X_OUTPUT_COLOR_SPEC" value="RGB1"/>
            <input name="IMAGE_TYPE" value="mcfo"/>
        </include>
        
    </sequence>
</process>