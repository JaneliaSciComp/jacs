<process name="DITP Alignment Pipeline" processor="queue/samplePipelineLauncher" startEvent="pending" updateProcessStatus="on_success">
    <sequence>

        <include name="Pipeline" process="FlyLightSamplePipeline">
            <input name="PIPELINE_NAME" value="DITP Pipeline"/>
            <input name="MERGE_ALGORITHMS" value="FLYLIGHT"/>
            <input name="STITCH_ALGORITHMS" value="FLYLIGHT"/>

            <!-- ALIGNMENT_ALGORITHMS comma separated values must match an enum from org.janelia.it.jacs.model.entity.cv.AlignmentAlgorithm -->
            <input name="ALIGNMENT_ALGORITHMS" value="CONFIGURED_BRAIN_VNC"/>

            <!-- ALIGNMENT_ALGORITHM_PARAMS identifies script in /groups/scicomp/jacsData/servers/[instance-name]/executables/install/scripts/brainaligner/ -->
            <input name="ALIGNMENT_ALGORITHM_PARAMS" value="flybrainaligner20x_ditp_JBA.sh"/>

            <input name="ANALYSIS_ALGORITHMS" value="NEURON_SEPARATOR"/>

            <!-- PIPELINE_PROCESS value must be in sync with PipelineConfig_[value].process file name and
                 should also be defined as an enum in org.janelia.it.jacs.model.entity.cv.PipelineProcess -->
            <input name="PIPELINE_PROCESS" value="DitpAlignment"/>
        </include>

    </sequence>
</process>