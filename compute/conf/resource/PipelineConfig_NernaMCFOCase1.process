<process name="Aljoscha MCFO Case 1 Pipeline" processor="queue/samplePipelineLauncher" startEvent="pending" updateProcessStatus="on_success">
    <sequence>

        <!--
          20X samples are paired lsms with
               lsm-1:    ch1: AF-488, ch2: AF-647 (or AF-633), chS1: DY-547 (or AF-546, AF-555, AF-568)
               lsm-2:    ch1: AF-488, ch2: AF-594

          63X samples are paired lsms with
               lsm-1:    ch1: AF-488, ch2: AF-647 (or AF-633), chS1: DY-547 (or AF-546, AF-555, AF-568)
               lsm-2:    ch1: AF-488, ch2: AF-594
        -->

        <include name="Pipeline" process="FlyLightPolarityPipeline">
            <input name="PIPELINE_PROCESS" value="NernaMCFOCase1"/>
            <input name="CHANNEL_DYE_SPEC_20X" value="membrane_ha=Alexa Fluor 633,Alexa Fluor 647;membrane_v5=Alexa Fluor 546,Alexa Fluor 555,Alexa Fluor 568,DY-547;membrane_flag=Alexa Fluor 594;reference=Alexa Fluor 488"/>
            <input name="OUTPUT_CHANNEL_ORDER_20X" value="membrane_ha,membrane_v5,membrane_flag,reference"/>
            <input name="CHANNEL_DYE_SPEC_63X" value="membrane_ha=Alexa Fluor 633,Alexa Fluor 647;membrane_v5=Alexa Fluor 546,Alexa Fluor 555,Alexa Fluor 568,DY-547;membrane_flag=Alexa Fluor 594;reference=Alexa Fluor 488"/>
            <input name="OUTPUT_CHANNEL_ORDER_63X" value="membrane_ha,membrane_v5,membrane_flag,reference"/>
        </include>
        
    </sequence>
</process>