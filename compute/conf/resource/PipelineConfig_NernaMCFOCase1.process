<process name="Aljoscha MCFO Case 1 Pipeline" processor="queue/samplePipelineLauncher" startEvent="pending" updateProcessStatus="on_success">
    <sequence>

        <!--
          post August 2013 20X samples are paired lsms with
               lsm-1:    dye-1: Alexa Fluor 488, dye-2: Alexa Fluor 647, dye-3: DY-547
               lsm-2:    dye-1: Alexa Fluor 488, dye-2: Alexa Fluor 594

          63X samples are paired lsms with
               lsm-1:    dye-1: Alexa Fluor 488, dye-2: Alexa Fluor 647, dye-3: DY-547
               lsm-2:    dye-1: Alexa Fluor 488, dye-2: Alexa Fluor 594
        -->

        <include name="Pipeline" process="FlyLightPolarityPipeline">
            <input name="PIPELINE_PROCESS" value="NernaMCFOCase1"/>
            <input name="20X_MERGE_ALGORITHMS" value="FLYLIGHT_ORDERED"/>
            <input name="CHANNEL_DYE_SPEC_20X" value="membrane_ha=Alexa Fluor 647;membrane_v5=DY-547,Alexa Fluor 568;membrane_flag=Alexa Fluor 594;reference=Alexa Fluor 488"/>
            <input name="OUTPUT_CHANNEL_ORDER_20X" value="membrane_ha,membrane_v5,membrane_flag,reference"/>
            <input name="CHANNEL_DYE_SPEC_63X" value="membrane_ha=Alexa Fluor 647;membrane_v5=DY-547,Alexa Fluor 568;membrane_flag=Alexa Fluor 594;reference=Alexa Fluor 488"/>
            <input name="OUTPUT_CHANNEL_ORDER_63X" value="membrane_ha,membrane_v5,membrane_flag,reference"/>
        </include>
        
    </sequence>
</process>