<!-- 
	Load in the screen pattern intensity scores.
-->
<process name="Mask Channel Compartments Loading Pipeline" processor="java:/jms/queue/largePipelineLauncher" startEvent="pending" updateProcessStatus="on_success">
	<sequence>
		 
	    <!-- Import variables -->
	    <operation name="Import Variables" processor="org.janelia.it.jacs.compute.service.common.InitVariablesFromTaskService">
            <input name="TASK_PARAMETER_1" value="mask chan file path"/>
            <input name="PROCESS_VARIABLE_1" value="MASK_CHAN_PATH"/>
            <input name="TASK_PARAMETER_2" value="top level folder name"/>
            <input name="PROCESS_VARIABLE_2" value="TOP_LEVEL_FOLDER_NAME"/>
            <input name="TASK_PARAMETER_3" value="optical resolution"/>
            <input name="PROCESS_VARIABLE_3" value="OPTICAL_RESOLUTION"/>
            <input name="TASK_PARAMETER_4" value="pixel resolution"/>
            <input name="PROCESS_VARIABLE_4" value="PIXEL_RESOLUTION"/>
            <input name="TASK_PARAMETER_5" value="alignment space name"/>
            <input name="PROCESS_VARIABLE_5" value="ALIGNMENT_SPACE_NAME"/>
	        <output name="MASK_CHAN_PATH"/>
	        <output name="TOP_LEVEL_FOLDER_NAME"/>
            <output name="OPTICAL_RESOLUTION"/>
            <output name="PIXEL_RESOLUTION"/>
            <output name="ALIGNMENT_SPACE_NAME"/>
	    </operation>
		 
	    <!-- Load everything and create entities as needed -->
		<operation name="Mask Chan Encoded Compartment Loading" processor="org.janelia.it.jacs.compute.service.fly.MaskChanCompartmentLoadingService">
			<input name="MASK_CHAN_PATH"/>
			<input name="TOP_LEVEL_FOLDER_NAME"/>
            <input name="OPTICAL_RESOLUTION"/>
            <input name="PIXEL_RESOLUTION"/>
            <input name="ALIGNMENT_SPACE_NAME"/>
		</operation>
		
    </sequence>
</process>