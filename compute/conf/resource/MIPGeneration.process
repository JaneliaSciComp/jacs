<process name="MIPService"  startEvent="pending" processor="queue/mipServiceLauncher" updateProcessStatus="on_success">
    <sequence name="Generate sample MIPs" waitForAsync="true">
        <operation name="Generate MIPs" processor="queue/gridSubmitAndWait">
            <input name="iservice" value="org.janelia.it.jacs.compute.service.image.BasicMIPandMovieGenerationService"/>
            <input name="RESULT_FILE_NODE" />
            <input name="RESULT_FILE_NODE_ID" />
            <input name="OUTPUT_FILE_NODE" />
            <input name="INPUT_IMAGES" />
            <input name="NORMALIZE_TO_FIRST_IMAGE" />
            <input name="OUTPUTS" />
        </operation>
    </sequence>
</process>
