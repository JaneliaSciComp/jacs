<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mbean PUBLIC
          "-//JBoss//DTD JBOSS XMBEAN 1.0//EN"
          "http://www.jboss.org/j2ee/dtd/jboss_xmbean_1_0.dtd">
<mbean>

    <description>Geometric Index Management Bean</description>
    <class>org.janelia.it.jacs.compute.mbean.GeometricIndexManager</class>
    <constructor>
        <description>The default constructor</description>
        <name>GeometricIndexManager</name>
    </constructor>
    
    <operation>
        <description>Start Geometric Index Manager</description>
        <name>startGeometricIndexManager</name>
    </operation>
    
    <operation>
        <description>Stop Geometric Index Manager</description>
        <name>stopGeometricIndexManager</name>
    </operation>
    
</mbean>
