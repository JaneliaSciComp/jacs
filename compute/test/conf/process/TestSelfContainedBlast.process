<process name="TestSelfContainedBlast">  <!-- Executed by ProcessLauncherSLSB -->
    <sequence>
        <operation name="Create Task" processor="compute/CreateBlastTaskSLSB/local"/>
    </sequence>
    <sequence processor="java:/jms/queue/blastLauncher">
        <operation name="Persit Query Node"  processor="org.jcvi.vics.compute.service.blast.persist.query.PersistQueryNodeService">
            <input name="USER_NAME" mandatory="true"/>
            <input name="FAST_TEXT" mandatory="true"/>
        </operation>
        <operation name="Create Blast Result File Node" processor="org.jcvi.vics.compute.service.blast.persist.results.initial.CreateBlastResultFileNodeService">
            <output name="RESULT_FILE_NODE_ID"/>
            <output name="RESULT_FILE_NODE_DIR" />
        </operation>
        <operation name="Submit Job" processor="org.jcvi.vics.compute.service.blast.submit.BlastSubmitJobService"/>
        <operation name="Merge Sort Results" processor="org.jcvi.vics.compute.service.blast.merge.MergeSortResultsService"/>
        <operation name="Persist Blast Results" processor="org.jcvi.vics.compute.service.blast.persist.results.finall.PersistFinalNodeResultsService" updateProcessStatus="true"/>
    </sequence>
</process>