<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.janelia.it.jacs.model.genomics"
                   default-access="property"
                   default-lazy="false"
                   auto-import="true"
                   default-cascade="none"
                  >

    <class name="SequenceType" table="sequence_type" mutable="false">

        <id name="code" type="java.lang.Integer" access="property"/>
        <property name="name" type="java.lang.String" not-null="true" column="name"/>
        <property name="description" type="java.lang.String" not-null="true" column="description"/>
        <property name="elements" type="java.lang.String" not-null="true" column="elements"/>
        <property name="complements" type="java.lang.String" not-null="true" column="complements"/>
        <property name="residueType" type="java.lang.String" not-null="true" column="residue_type"/>
    </class>

</hibernate-mapping>