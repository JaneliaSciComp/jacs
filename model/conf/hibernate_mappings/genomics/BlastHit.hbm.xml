<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.janelia.it.jacs.model.genomics"
                   default-access="property"
                   default-lazy="false"
                   auto-import="true"
                   default-cascade="none"
                  >
       <class name="BlastHit" lazy="true" table="blast_hit">
           <id name="blastHitId"
               type="java.lang.Long"
               access="field"
               column="blast_hit_id"
              >
               <generator class="org.janelia.it.jacs.model.TimebasedIdentifierGenerator" />
           </id>
           <many-to-one name="subjectEntity" class="BaseSequenceEntity" cascade="save-update"
                        column="subject_acc" property-ref="accession" not-found="ignore"
                        insert="false" update="false" lazy="proxy"/>
           <property name="subjectAcc" type="java.lang.String" column="subject_acc" not-null="true"
                     insert="true" update="true"/>
           <property name="subjectBegin" type="java.lang.Integer" column="subject_begin" not-null="true" />
           <property name="subjectEnd" type="java.lang.Integer" column="subject_end" not-null="true"/>
           <property name="subjectOrientation" type="java.lang.Integer" column="subject_orientation" not-null="true" />
           <many-to-one name="queryEntity" class="BaseSequenceEntity" cascade="save-update"
                        column="query_acc" property-ref="accession" not-found="ignore"
                        insert="false" update="false" lazy="proxy"/>
           <property name="queryNodeId" type="java.lang.Long" column="query_node_id" />
           <property name="queryAcc" type="java.lang.String" column="query_acc" not-null="true"
                     insert="true" update="true"/>
           <property name="queryBegin" type="java.lang.Integer" column="query_begin" not-null="true" />
           <property name="queryEnd" type="java.lang.Integer" column="query_end" not-null="true" />
           <property name="queryOrientation" type="java.lang.Integer" column="query_orientation" not-null="true" />
           <many-to-one name="resultNode" class="org.janelia.it.jacs.model.user_data.Node" cascade="all"
                        column="result_node_id" lazy="proxy"/>
           <property name="rank" type="java.lang.Integer" column="result_rank"/>
           <property name="programUsed" type="java.lang.String" column="program_used" not-null="true" />
           <property name="blastVersion" type="java.lang.String" column="blast_version"/>
           <property name="bitScore" type="java.lang.Float" column="bit_score" />
           <property name="hspScore" type="java.lang.Float" column="hsp_score" />
           <property name="expectScore" type="java.lang.Double" column="expect_score" />
           <property name="comment" type="java.lang.String" column="comment" />
           <property name="lengthAlignment" type="java.lang.Integer" column="length_alignment" />
           <property name="entropy" type="java.lang.Float" column="entropy" />
           <property name="numberIdentical" type="java.lang.Integer" column="number_identical" />
           <property name="numberSimilar" type="java.lang.Integer" column="number_similar" />
           <property name="subjectLength" type="java.lang.Integer" column="subject_length" />
           <property name="subjectGaps" type="java.lang.Integer" column="subject_gaps" />
           <property name="subjectGapRuns" type="java.lang.Integer" column="subject_gap_runs" />
           <property name="subjectStops" type="java.lang.Integer" column="subject_stops" />
           <property name="subjectNumberUnalignable" type="java.lang.Integer" column="subject_number_unalignable" />
           <property name="subjectFrame" type="java.lang.Integer" column="subject_frame" />
           <property name="queryLength" type="java.lang.Integer" column="query_length" />
           <property name="queryGaps" type="java.lang.Integer" column="query_gaps" />
           <property name="queryGapRuns" type="java.lang.Integer" column="query_gap_runs" />
           <property name="queryStops" type="java.lang.Integer" column="query_stops" />
           <property name="queryNumberUnalignable" type="java.lang.Integer" column="query_number_unalignable" />
           <property name="queryFrame" type="java.lang.Integer" column="query_frame" />
           <property name="subjectAlignString" type="java.lang.String" column="subject_align_string" />
           <property name="midline" type="java.lang.String" column="midline_align_string" />
           <property name="queryAlignString" type="java.lang.String" column="query_align_string" />
       </class>
</hibernate-mapping>
