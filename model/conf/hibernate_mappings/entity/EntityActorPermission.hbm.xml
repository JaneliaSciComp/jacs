<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="flyportal"
				   package="org.janelia.it.jacs.model.entity"
                   >
    <class name="EntityActorPermission" table="entity_actor_permission">
        <id name="id" column="id" type="java.lang.Long">
            <generator class="org.janelia.it.jacs.model.TimebasedIdentifierGenerator" />
        </id>
        <many-to-one name="entity" class="Entity" cascade="none" fetch="select" lazy="proxy">
            <column name="entity_id" length="60" />
        </many-to-one>
        <property name="subjectKey" type="string">
            <column name="subject_key" length="64" />
        </property>
        <property name="permissions" type="string">
            <column name="permissions" length="8" />
        </property>
    </class>
</hibernate-mapping>
