<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.janelia.it.jacs.model.entity">
    <class name="EntityData" table="entityData" >
        <id name="id" column="id" type="java.lang.Long">
            <generator class="org.janelia.it.jacs.model.TimebasedIdentifierGenerator" />
        </id>
        <property name="entityAttrName" type="java.lang.String" not-null="true">
            <column name="entity_att" length="60" />
        </property>
        <many-to-one name="parentEntity" class="Entity" cascade="none" fetch="select" lazy="proxy">
            <column name="parent_entity_id" length="60" />
        </many-to-one>
        <many-to-one name="childEntity" class="Entity" cascade="none" fetch="select" lazy="proxy">
            <column name="child_entity_id" length="60" />
        </many-to-one>
        <property name="ownerKey" type="java.lang.String">
            <column name="owner_key" length="64" />
        </property>
        <property name="value" type="java.lang.String">
            <column name="value" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" length="19" />
        </property>
        <property name="updatedDate" type="timestamp">
            <column name="updated_date" length="19" />
        </property>
        <property name="orderIndex" type="java.lang.Integer">
            <column name="orderIndex" length='11' />
        </property>
    </class>
</hibernate-mapping>
