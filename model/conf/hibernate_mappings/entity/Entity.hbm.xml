<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.janelia.it.jacs.model.entity">
    <class name="Entity" table="entity">
        <id name="id" column="id" type="java.lang.Long">
            <generator class="org.janelia.it.jacs.model.TimebasedIdentifierGenerator" />
        </id>
        <property name="name" type="string">
            <column name="name" length="4000" />
        </property>
        <property name="ownerKey" type="string">
            <column name="owner_key" length="64" />
        </property>
        <property name="entityTypeName" type="string">
            <column name="entity_type" not-null="true" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" length="19" />
        </property>
        <property name="updatedDate" type="timestamp">
            <column name="updated_date" length="19" />
        </property>
        <property name="numChildren" type="java.lang.Integer">
            <column name="num_children" length="11" />
        </property>
        <set name="entityData" cascade="all" inverse="true" lazy="true" batch-size="100" table="entityData">
            <key column="parent_entity_id" not-null="true"/>
            <one-to-many class="EntityData" />
        </set>
        <set name="entityActorPermissions" cascade="all,delete-orphan" inverse="true" lazy="true" table="entity_actor_permission">
            <key column="entity_id" not-null="true"/>
            <one-to-many class="EntityActorPermission" />
        </set>
    </class>
</hibernate-mapping>
