<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping schema="flyportal"
                   package="org.janelia.it.jacs.model.user_data"
                   default-access="property"
                   default-lazy="false"
                   auto-import="true"
                   default-cascade="none"
                  >
    <class name="org.janelia.it.jacs.model.user_data.DataSource" table="data_source"
           lazy="false">
        <id name="sourceId" access="field" column="source_id" type="java.lang.Long">
            <generator class="org.janelia.it.jacs.model.TimebasedIdentifierGenerator" />
        </id>
        <property name="sourceName" unique="true" not-null="true"
            column="source_name" type="string" index="data_source_key_source_name"/>
        <property name="dataVersion" type="string" column="data_version"/>
    </class>

</hibernate-mapping>