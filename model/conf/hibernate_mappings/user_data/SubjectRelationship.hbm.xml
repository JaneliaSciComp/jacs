<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="flyportal"
                   package="org.janelia.it.jacs.model.user_data"
                  >
    <class name="org.janelia.it.jacs.model.user_data.SubjectRelationship" table="subject_relationship" lazy="true">

        <id name="id" access="field" column="id" type="java.lang.Long">
            <generator class="org.janelia.it.jacs.model.TimebasedIdentifierGenerator" />
        </id>

        <many-to-one name="user" class="User" cascade="none" fetch="select" lazy="proxy">
            <column name="user_subject_id" />
        </many-to-one>
        
        <many-to-one name="group" class="Group" cascade="none" fetch="select" lazy="proxy">
            <column name="group_subject_id" />
        </many-to-one>
        
        <property name="relationshipType" column="relationship_type" type="string"/>
        
    </class>

</hibernate-mapping>