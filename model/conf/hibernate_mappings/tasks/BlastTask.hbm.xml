<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.janelia.it.jacs.model.tasks.blast"
                   default-access="property"
                   default-lazy="false"
                   auto-import="true"
                   default-cascade="none" >
        <subclass name="BlastTask" extends="org.janelia.it.jacs.model.tasks.Task"
                  discriminator-value="blast" abstract="true">
            <subclass name="BlastNTask" discriminator-value="blastn" lazy="false" >
                <subclass name="TeraBlastNTask" discriminator-value="tera-blastn" lazy="false" />
            </subclass>
            <subclass name="MegablastTask" discriminator-value="megablast" lazy="false" />
            <subclass name="TBlastNTask" discriminator-value="tblastn" lazy="false" >
                <subclass name="TeraTBlastNTask" discriminator-value="tera-tblastn" lazy="false" />
            </subclass>
            <subclass name="BlastPTask" discriminator-value="blastp" lazy="false" >
                <subclass name="TeraBlastPTask" discriminator-value="tera-blastp" lazy="false"/>
            </subclass>
            <subclass name="BlastXTask" discriminator-value="blastx" lazy="false" >
                <subclass name="TeraBlastXTask" discriminator-value="tera-blastx" lazy="false" />
            </subclass>
            <subclass name="TBlastXTask" discriminator-value="tblastx" lazy="false" >
                <subclass name="TeraTBlastXTask" discriminator-value="tera-tblastx" lazy="false"/>
            </subclass>
            <subclass name="org.janelia.it.jacs.model.tasks.psiBlast.PsiBlastTask"
                  discriminator-value="blastpgp" lazy="false"/>
            <subclass name="TeragridSimpleBlastTask" discriminator-value="tgsimpleblast" lazy="false" />
        </subclass>
</hibernate-mapping>