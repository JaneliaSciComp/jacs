<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.janelia.it.jacs.model.tasks.export"
                   default-access="property"
                   default-lazy="false"
                   auto-import="true"
                   default-cascade="none" >
        <subclass name="ExportTask" extends="org.janelia.it.jacs.model.tasks.Task"
                  discriminator-value="export">
            <subclass name="org.janelia.it.jacs.model.tasks.export.BlastResultExportTask"
                      discriminator-value="blastResultExportTask" lazy="false" >
            </subclass>
            <subclass name="org.janelia.it.jacs.model.tasks.export.ClusterProteinAnnotationExportTask"
                      discriminator-value="clusterProteinAnnotationExportTask" lazy="false" >
            </subclass>
            <!--<subclass name="org.janelia.it.jacs.model.tasks.export.ClusterSearchExportTask"-->
                      <!--discriminator-value="clusterSearchExportTask" lazy="false" >-->
            <!--</subclass>-->
            <!--<subclass name="org.janelia.it.jacs.model.tasks.export.FrvReadExportTask"-->
                      <!--discriminator-value="frvReadExportTask" lazy="false" >-->
            <!--</subclass>-->
            <!--<subclass name="org.janelia.it.jacs.model.tasks.export.ProteinSearchExportTask"-->
                      <!--discriminator-value="proteinSearchExportTask" lazy="false" >-->
            <!--</subclass>-->
            <subclass name="org.janelia.it.jacs.model.tasks.export.SampleExportTask"
                      discriminator-value="sampleExportTask" lazy="false" >
            </subclass>
            <subclass name="org.janelia.it.jacs.model.tasks.export.SequenceExportTask"
                      discriminator-value="sequenceExportTask" lazy="false" >
            </subclass>
            <subclass name="org.janelia.it.jacs.model.tasks.export.FileNodeExportTask"
                      discriminator-value="fileNodeExportTask" lazy="false" >
            </subclass>
        </subclass>
</hibernate-mapping>
