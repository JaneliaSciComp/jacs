<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.janelia.it.jacs.model.tasks.tic"
                   default-access="property"
                   default-lazy="false"
                   auto-import="true"
                   default-cascade="none" >
        <subclass name="TicTask" extends="org.janelia.it.jacs.model.tasks.Task"
                  discriminator-value="ticTask" abstract="true">
            <subclass name="BatchTicTask" discriminator-value="batchTICTask" abstract="false"/>
            <subclass name="SingleTicTask" discriminator-value="singleTICTask" abstract="false"/>
        </subclass>

</hibernate-mapping>
